{{#site_footer}}
&lt;div class=&quot;footer__wrap page-container&quot;&gt;
  &lt;div class=&quot;footer__tout-row&quot;&gt;
    &lt;div class=&quot;footer__tout-row-interior&quot;&gt;
      {{#touts}}
      &lt;div class=&quot;footer__tout&quot;&gt;
        &lt;div class=&quot;footer__tout-header&quot;&gt;
          &lt;div class=&quot;footer__tout__text-1&quot;&gt;{{title-1}}&lt;/div&gt;
          &lt;div class=&quot;footer__tout__text-2&quot;&gt;{{title-2}}&lt;/div&gt;
          &lt;div class=&quot;footer__tout__text-3&quot;&gt;{{text}}&lt;/div&gt;
        &lt;/div&gt;
        &lt;div class=&quot;footer__tout-interior&quot;&gt;
          {{#is_newsletter_signup?}}
          &lt;div class=&quot;footer__tout-form&quot;&gt;
            &lt;form action=&quot;#&quot;&gt;
              &lt;input type=&quot;text&quot; name=&quot;email&quot; placeholder=&quot;Email&quot;&gt;
              &lt;input type=&quot;text&quot; name=&quot;zipcode&quot; placeholder=&quot;Zipcode&quot;&gt;
            &lt;/form&gt;
          &lt;/div&gt;
          {{/is_newsletter_signup?}}
          {{^is_newsletter_signup?}}
          &lt;div class=&quot;footer__tout-image&quot;&gt;
            &lt;img src=&quot;{{img}}&quot;&gt;
          &lt;/div&gt;
          {{/is_newsletter_signup?}}
        &lt;/div&gt;
        &lt;div class=&quot;footer__tout-button&quot;&gt;
          &lt;a href=&quot;{{url}}&quot; class=&quot;btn&quot;&gt;{{link_text}}&lt;/a&gt;
        &lt;/div&gt;
      &lt;/div&gt;
      {{/touts}}
    &lt;/div&gt;
  &lt;/div&gt;
  &lt;div class=&quot;footer&quot;&gt;
    &lt;div class=&quot;footer-top&quot;&gt;
      &lt;div class=&quot;footer__logo&quot;&gt;
        &lt;svg class=&quot;footer__logo-icon&quot;&gt;
          &lt;use xlink:href='../../assets/img/symbols.svg#icon-essence-logo--slideout'&gt;&lt;/use&gt;
        &lt;/svg&gt;
        &lt;div class=&quot;social-list social-list--large social-list--white module-name-etr_social_follow_us&quot;&gt;
          &lt;div class=&quot;social-list__pretext&quot;&gt;{{social_pretext}}&lt;/div&gt;
          &lt;div class=&quot;social-list__icons&quot;&gt;
            &lt;a target=&quot;_blank&quot; class=&quot;social-list__item&quot; href=&quot;#&quot;&gt;
              &lt;svg class=&quot;social-list__icon social-list__icon--facebook&quot;&gt;
                &lt;use xlink:href=&quot;../../assets/img/symbols.svg#icon-facebook&quot;&gt;&lt;/use&gt;
              &lt;/svg&gt;
            &lt;/a&gt;
            &lt;a target=&quot;_blank&quot; class=&quot;social-list__item&quot; href=&quot;#&quot;&gt;
              &lt;svg class=&quot;social-list__icon social-list__icon--twitter&quot;&gt;
                &lt;use xlink:href=&quot;../../assets/img/symbols.svg#icon-twitter&quot;&gt;&lt;/use&gt;
              &lt;/svg&gt;
            &lt;/a&gt;
            &lt;a target=&quot;_blank&quot; class=&quot;social-list__item&quot; href=&quot;#&quot;&gt;
              &lt;svg class=&quot;social-list__icon social-list__icon--pinterest&quot;&gt;
                &lt;use xlink:href=&quot;../../assets/img/symbols.svg#icon-pinterest&quot;&gt;&lt;/use&gt;
              &lt;/svg&gt;
            &lt;/a&gt;
            &lt;a target=&quot;_blank&quot; class=&quot;social-list__item&quot; href=&quot;#&quot;&gt;
              &lt;svg class=&quot;social-list__icon social-list__icon--video&quot;&gt;
                &lt;use xlink:href=&quot;../../assets/img/symbols.svg#icon-video&quot;&gt;&lt;/use&gt;
              &lt;/svg&gt;
            &lt;/a&gt;
            &lt;a target=&quot;_blank&quot; class=&quot;social-list__item&quot; href=&quot;#&quot;&gt;
              &lt;svg class=&quot;social-list__icon social-list__icon--instagram&quot;&gt;
                &lt;use xlink:href=&quot;../../assets/img/symbols.svg#icon-instagram&quot;&gt;&lt;/use&gt;
              &lt;/svg&gt;
            &lt;/a&gt;
            &lt;a target=&quot;_blank&quot; class=&quot;social-list__item&quot; href=&quot;#&quot;&gt;
              &lt;svg class=&quot;social-list__icon social-list__icon--email&quot;&gt;
                &lt;use xlink:href=&quot;../../assets/img/symbols.svg#icon-google-plus&quot;&gt;&lt;/use&gt;
              &lt;/svg&gt;
            &lt;/a&gt;
          &lt;/div&gt;
        &lt;/div&gt;
      &lt;/div&gt;
      &lt;div class=&quot;footer__list-wrap&quot;&gt;
        {{#link_lists}}
        &lt;div class=&quot;footer__list&quot;&gt;
          &lt;div class=&quot;footer__list-title&quot;&gt;{{list_title}}&lt;/div&gt;
            &lt;ul class=&quot;footer__list-container&quot;&gt;
              {{#links}}
              &lt;li class=&quot;footer__list-item&quot;&gt;&lt;a href=&quot;{{url}}&quot;&gt;{{link_text}}&lt;/a&gt;&lt;/li&gt;
              {{/links}}
            &lt;/ul&gt;
        &lt;/div&gt;
        {{/link_lists}}
      &lt;/div&gt;
    &lt;/div&gt;
    &lt;div class=&quot;footer-bottom&quot;&gt;
      &lt;div class=&quot;footer-bottom__legal-text&quot;&gt;{{legal_text_1}}&lt;/div&gt;
      &lt;div class=&quot;footer-bottom__legal-text&quot;&gt;{{{legal_text_2}}}&lt;/div&gt;
    &lt;/div&gt;
  &lt;/div&gt;
&lt;/div&gt;
{{/site_footer}}